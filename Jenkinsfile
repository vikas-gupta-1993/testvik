pipeline {
    agent any
    stages {
        stage("init") {
            steps {
                echo "Hello World init!"
            }
        }
        stage("build") {
            steps {
                echo "Hello World build!"
            }
        }
        stage("test") {
            steps {
                echo "Hello World test!"
            }
        }
        stage("deploy") {
            steps {
                echo "Hello World deploy!"
            }
        }
    }   
}